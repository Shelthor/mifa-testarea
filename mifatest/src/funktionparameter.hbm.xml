<!DOCTYPE hibernate-mapping PUBLIC  
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
      
     <hibernate-mapping>  
      <class name="com.mifatest.entities.FunktionParameter" table="funktion_parameter">  
        <id name="funktion_parameterID">  
         <generator class="increment"></generator>  
        </id>        
        
        <many-to-one name="parameterID" class="com.mifatest.entities.Parameter"
            column="parameterID" unique="false" not-null="true"
            cascade="all" />
        <many-to-one name="funktionID" class="com.mifatest.entities.Funktion"
            column="funktionID" unique="false" not-null="true"
            cascade="all" />   
                
      </class>  
                
     </hibernate-mapping>  